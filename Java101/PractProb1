public class PractProb1 {
    public static void main(String[] args) {
        int[] arr= {1,3,6,4,4,3,6,7,4};
        int n=4;

        System.out.println(countOccurrences(arr,n));

    }

        public static int countOccurrences (int[] arr, int n) {
            int numOfn = 0;
            for (int number : arr ) {
                if (number == n)
                    numOfn ++;
            }
            return numOfn;
            
        }   
}